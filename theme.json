{
    "$schema": "https://schemas.wp.org/wp/6.4/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "color": {
            "defaultPalette": false,
            "palette": [
                {
                    "slug": "primary",
                    "color": "#1B4332",
                    "name": "Deep Forest Green"
                },
                {
                    "slug": "secondary",
                    "color": "#2D6A4F",
                    "name": "Teal"
                },
                {
                    "slug": "accent",
                    "color": "#D4A574",
                    "name": "Mustard Yellow"
                },
                {
                    "slug": "charcoal",
                    "color": "#212529",
                    "name": "Charcoal"
                },
                {
                    "slug": "purple",
                    "color": "#6C5CE7",
                    "name": "Purple"
                },
                {
                    "slug": "base",
                    "color": "#FFFFFF",
                    "name": "White"
                },
                {
                    "slug": "contrast",
                    "color": "#212529",
                    "name": "Charcoal Dark"
                },
                {
                    "slug": "muted",
                    "color": "#6C757D",
                    "name": "Medium Gray"
                },
                {
                    "slug": "light",
                    "color": "#F8F9FA",
                    "name": "Light Gray"
                }
            ],
            "gradients": [
                {
                    "slug": "primary-gradient",
                    "gradient": "linear-gradient(90deg, #1B4332 0%, #2D6A4F 50%, #D4A574 100%)",
                    "name": "HTP Primary Gradient"
                },
                {
                    "slug": "hero-background",
                    "gradient": "linear-gradient(135deg, #F0F4F2 0%, #E8F0EE 50%, #FFF8ED 100%)",
                    "name": "HTP Hero Background"
                },
                {
                    "slug": "footer-gradient",
                    "gradient": "linear-gradient(90deg, #212529 0%, #2D6A4F 50%, #212529 100%)",
                    "name": "HTP Footer Gradient"
                }
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
                    "slug": "primary-font",
                    "name": "Inter (Body)"
                },
                {
                    "fontFamily": "'Montserrat', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
                    "slug": "heading-font",
                    "name": "Montserrat (Headings)"
                },
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
                    "slug": "system-font",
                    "name": "System Font"
                }
            ],
            "fontSizes": [
                {
                    "slug": "small",
                    "size": "0.875rem",
                    "name": "Small"
                },
                {
                    "slug": "normal",
                    "size": "1rem",
                    "name": "Normal"
                },
                {
                    "slug": "medium",
                    "size": "1.125rem",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "1.5rem",
                    "name": "Large"
                },
                {
                    "slug": "x-large",
                    "size": "2rem",
                    "name": "Extra Large"
                },
                {
                    "slug": "xx-large",
                    "size": "3rem",
                    "name": "2X Large"
                },
                {
                    "slug": "huge",
                    "size": "4rem",
                    "name": "Huge"
                }
            ]
        },
        "spacing": {
            "units": ["px", "em", "rem", "%", "vw", "vh"],
            "padding": true,
            "margin": true,
            "blockGap": true
        },
        "layout": {
            "contentSize": "1200px",
            "wideSize": "1400px"
        },
        "custom": {
            "spacing": {
                "small": "1rem",
                "medium": "2rem",
                "large": "4rem"
            }
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--base)",
            "text": "var(--wp--preset--color--contrast)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--primary-font)",
            "fontSize": "var(--wp--preset--font-size--normal)",
            "lineHeight": "1.6"
        },
        "spacing": {
            "padding": {
                "top": "0",
                "right": "0",
                "bottom": "0",
                "left": "0"
            },
            "margin": {
                "top": "0",
                "right": "0",
                "bottom": "0",
                "left": "0"
            }
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--secondary)"
                    }
                }
            },
            "button": {
                "border": {
                    "radius": "8px"
                },
                "color": {
                    "background": "var(--wp--preset--gradient--primary-gradient)",
                    "text": "var(--wp--preset--color--base)"
                },
                "typography": {
                    "fontWeight": "600"
                },
                ":hover": {
                    "transform": "scale(1.05)"
                }
            },
            "h1": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-font)",
                    "fontSize": "var(--wp--preset--font-size--huge)",
                    "fontWeight": "700",
                    "lineHeight": "1.2"
                }
            },
            "h2": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-font)",
                    "fontSize": "var(--wp--preset--font-size--xx-large)",
                    "fontWeight": "600",
                    "lineHeight": "1.3"
                }
            },
            "h3": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-font)",
                    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "fontWeight": "600",
                    "lineHeight": "1.4"
                }
            },
            "h4": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-font)",
                    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "600",
                    "lineHeight": "1.4"
                }
            },
            "h5": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-font)",
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "600",
                    "lineHeight": "1.5"
                }
            },
            "h6": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-font)",
                    "fontSize": "var(--wp--preset--font-size--normal)",
                    "fontWeight": "600",
                    "lineHeight": "1.5"
                }
            }
        },
        "blocks": {
            "core/navigation": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--normal)"
                }
            },
            "core/site-title": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "700"
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ],
    "customTemplates": [
        {
            "name": "home",
            "title": "Home",
            "postTypes": ["page"]
        },
        {
            "name": "page-no-header",
            "title": "Page (No Header)",
            "postTypes": ["page"]
        },
        {
            "name": "blank",
            "title": "Blank",
            "postTypes": ["page", "post"]
        },
        {
            "name": "dashboard",
            "title": "Dashboard",
            "postTypes": ["page"]
        },
        {
            "name": "login",
            "title": "Login",
            "postTypes": ["page"]
        },
        {
            "name": "page-compliance",
            "title": "Compliance",
            "postTypes": ["page"]
        },
        {
            "name": "page-privacy-policy",
            "title": "Privacy Policy",
            "postTypes": ["page"]
        },
        {
            "name": "page-terms",
            "title": "Terms of Service",
            "postTypes": ["page"]
        },
        {
            "name": "page-partner-portal",
            "title": "Partner Portal",
            "postTypes": ["page"]
        },
        {
            "name": "page-api-docs",
            "title": "API Documentation",
            "postTypes": ["page"]
        }
    ]
}